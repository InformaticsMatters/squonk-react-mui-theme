{
  "name": "@squonk/mui-theme",
  "version": "1.0.0-dev.1",
  "description": "Material-UI theme used on Squonk products",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "repository": "github:InformaticsMatters/react-sci-components",
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://github.com/InformaticsMatters/squonk-react-mui-theme/",
  "keywords": [
    "react",
    "material-ui"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "type-check": "tsc --pretty --noEmit",
    "release": "release-it",
    "prepublish": "tsup",
    "prepare": "husky install",
    "lint": "eslint . --fix"
  },
  "peerDependencies": {
    "react": ">=17",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "release-it": "^14.10.0",
    "styled-components": "^5.3.0",
    "tslib": "^2.3.0",
    "tsup": "^4.12.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "npm run lint"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    }
  }
}
