{
  "name": "@squonk/mui-theme",
  "version": "3.0.1",
  "type": "module",
  "description": "MUI theme used on Squonk products",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": "github:InformaticsMatters/squonk-react-mui-theme",
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://github.com/InformaticsMatters/squonk-react-mui-theme/",
  "scripts": {
    "prepare": "husky install",
    "build": "tsup",
    "start": "tsup --watch",
    "tsc": "tsc --pretty --noEmit",
    "release": "tsup && release-it",
    "format": "eslint . --fix"
  },
  "peerDependencies": {
    "@mui/material": ">5"
  },
  "devDependencies": {
    "@mui/material": "5.8.7",
    "@squonk/eslint-config": "0.4.2",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "release-it": "14.11.8",
    "tslib": "2.4.0",
    "tsup": "6.3.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "pnpm format"
    ]
  },
  "release-it": {
    "git": {
      "commit": true,
      "commitMessage": "chore: release v${version}",
      "tag": true,
      "push": true
    },
    "npm": {
      "publish": true
    }
  }
}
