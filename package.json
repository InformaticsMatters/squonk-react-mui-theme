{
  "name": "@squonk/mui-theme",
  "version": "2.0.0",
  "description": "Material-UI theme used on Squonk products",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": "github:InformaticsMatters/squonk-react-mui-theme",
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://github.com/InformaticsMatters/squonk-react-mui-theme/",
  "keywords": [
    "react",
    "material-ui"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "type-check": "tsc --pretty --noEmit",
    "release": "tsup && release-it",
    "prepublish": "tsup",
    "prepare": "husky install",
    "lint": "eslint . --fix"
  },
  "peerDependencies": {
    "@material-ui/core": "4.12.3"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "release-it": "^14.11.5",
    "tslib": "^2.3.1",
    "tsup": "^4.14.0",
    "typescript": "^4.4.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "npm run lint"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    }
  }
}
