{
  "name": "@squonk/mui-theme",
  "version": "2.0.1",
  "type": "module",
  "description": "Material-UI theme used on Squonk products",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": "github:InformaticsMatters/squonk-react-mui-theme",
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://github.com/InformaticsMatters/squonk-react-mui-theme/",
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "type-check": "tsc --pretty --noEmit",
    "release": "tsup && release-it",
    "lint": "eslint . --fix"
  },
  "peerDependencies": {
    "@material-ui/core": ">4.12"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.3",
    "@squonk/eslint-config": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "release-it": "^14.11.8",
    "tslib": "^2.3.1",
    "tsup": "^5.11.1",
    "typescript": "^4.5.3"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "npm run lint"
    ]
  },
  "release-it": {
    "git": {
      "commit": true,
      "commitMessage": "chore: release v${version}",
      "tag": true,
      "push": true
    },
    "npm": {
      "publish": true
    }
  }
}
