{
  "name": "@squonk/mui-theme",
  "version": "5.0.0",
  "type": "module",
  "description": "MUI theme used on Squonk products",
  "module": "./src/index.ts",
  "typings": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "typings": "./dist/index.d.ts"
  },
  "sideEffects": false,
  "repository": "github:InformaticsMatters/squonk-react-mui-theme",
  "license": "Apache-2.0",
  "private": false,
  "homepage": "https://github.com/InformaticsMatters/squonk-react-mui-theme/",
  "scripts": {
    "prepare": "husky install",
    "build": "tsup",
    "start": "tsup --watch",
    "tsc": "tsc --pretty --noEmit",
    "lint": "eslint --cache --max-warnings=0",
    "format": "eslint --fix --cache --max-warnings=0",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "@mui/material": ">5"
  },
  "devDependencies": {
    "@mui/material": "7.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@squonk/eslint-config": "2.1.0",
    "eslint": "8.57.1",
    "husky": "8.0.1",
    "lint-staged": "15.5.0",
    "semantic-release": "24.2.3",
    "tslib": "2.8.1",
    "tsup": "8.4.0",
    "typescript": "5.8.2"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "pnpm format ."
    ]
  }
}
